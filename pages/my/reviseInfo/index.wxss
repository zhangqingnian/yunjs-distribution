/* pages/my/reviseInfo/index.wxss */
page{
    width: 100%;
    height: 100%;
    
}
.containner{
    box-sizing: border-box;
    padding-bottom: 120rpx;
}
.fontSize24{
    font-size: 24rpx;
}
.fontSize20{
    font-size: 20rpx;
}
.between{
    justify-content: space-between;
}
.containner{
    width: 100%;
    min-height: 100%;
    background: #F5F5F5;
    color: #323232;
    font-size: 30rpx;
}

.status{
    color: #fff;
    width:140rpx;
    height:50rpx;
    background:#EDC237;
    border-radius:20rpx;
    text-align: center;
    line-height: 50rpx;
}

.baseInfo .top{
    padding: 20rpx 30rpx;
}

.list{
    background: #fff;
    padding: 0 30rpx;
}

.list .item{
    border-bottom: 1px solid #DCDCDC;
    padding: 0 30rpx;
}

.input{
    width: 500rpx;
    height: 70rpx;
    padding-left: 20rpx;
    box-sizing: border-box;
    text-align: right;
    font-size: 28rpx;
    color: #969696;
}




.qualification{
    margin-top: 30rpx;
}

.heading{
    margin-bottom: 20rpx;
}

.qualification .fontSize24{
    color: #969696;
}

.example{
    width:70rpx;
    height:40rpx;
    background:rgba(245,245,245,1);
    border:2rpx solid rgba(99,175,224,1);
    text-align: center;
    line-height: 40rpx;
    color: #63AFE0;
}

.add{
    width: 32rpx;
    height: 32rpx;
    margin-right: 10rpx;
}

.uploadWarp{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.upload{
    width: 100%;
    font-size: 36rpx;
    background: #63AFE0;
    color: #fff;
    height: 88rpx;
    line-height: 88rpx;
    justify-content: center;
    margin-top: 20rpx;
}

.uploadImg{
    width: 350rpx;
    height: 220rpx;
    margin-top: 20rpx;
    background: #EDC237;
}